<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.project2.model.User" table="USER_TABLE">
		<id name="userId" column="USER_ID">
			<generator class="increment"></generator>
		</id>
		<property name="name" type="org.hibernate.type.StringType" column="USER_NAME"></property>
		<property name="email" type="org.hibernate.type.StringType" column="USER_EMAIL"></property>
		<property name="username" type="org.hibernate.type.StringType" column="USERNAME"></property>
		<property name="password" type="org.hibernate.type.StringType" column="USER_PASSWORD"></property>
		<property name="isManager" type="org.hibernate.type.IntegerType" column="IS_MANAGER"></property>
		<property name="income" type="org.hibernate.type.LongType" column="USER_INCOME"></property>
	</class>
</hibernate-mapping>